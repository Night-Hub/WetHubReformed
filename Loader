--VERSION 0.3

------------------------------------------------------------------------------------------------------------ Making Sure WetHub Has Not Already Been Executed
for i, v in pairs(game.CoreGui:GetDescendants()) do
	if v.Name == "WetHub" or v.Name == "WetHub - Grow A Garden" then
		warn("You've Already Executed WetHub!")
		print("Sending Notification")
		game.StarterGui:SetCore("SendNotification",
		{ Title = "WetHub";
		Text = "Script has already been executed!";
		Icon = "rbxthumb://type=Asset&id=6845502547&w=150&h=150";
		Duration = 10;
		Button1 = "Okay";
		Callback = NotificationBindable; })
		print("Notification Sent!")
		while true do
			task.wait()
		end
	end
end
------------------------------------------------------------------------------------------------------------ Main Loadstrings For WetHub
if game.PlaceId == 5272267802 then -- One -- BeAToy
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Night-Hub/WetHubReformed/refs/heads/main/BeAToy"))()
	elseif game.PlaceId == 126884695634066 then -- Two -- Grow A Garden
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Night-Hub/WetHubReformed/refs/heads/main/GrowAGarden"))()
	elseif game.PlaceId == 537413528 then -- Three -- BABFT
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Night-Hub/WetHubReformed/refs/heads/main/BABFT"))()
	elseif game.PlaceId == 2986677229 then -- Four -- Nil
		loadstring(game:HttpGet("--"))()
	elseif game.PlaceId == 28609042 then
		--loadstring
	end
------------------------------------------------------------------------------------------------------------
